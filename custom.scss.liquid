/* Fallback for browsers that don't support -webkit-line-clamp */
.line-clamp {
    position: relative;
    overflow: hidden;
    max-height: 2.8em; /* set this to line height * 2 */
}

.line-clamp:after {
    content: "";
    text-align: right;
    position: absolute;
    bottom: 0;
    right: 0;
    width: 100%;
    background: linear-gradient(to right, rgba(255, 255, 255, 0), rgba(255, 255, 255, 1) 80%);
}

/* Code for the browsers that support -webkit-line-clamp and overwrite the above code */
@supports (-webkit-line-clamp: 2) {
    .line-clamp {
        overflow: hidden;
        text-overflow: ellipsis;
        display: -webkit-box;
        -webkit-line-clamp: 2;
        -webkit-box-orient: vertical;
    }

    .line-clamp:after {
        display: none;
    }
}


